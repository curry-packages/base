{
    "name": "base",
    "version": "3.2.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Base libraries for Curry systems",
    "description": "This package contains the base libraries which are directly distributed with Curry systems like PAKCS, KiCS2, or Curry2Go.",
    "category": [ "Programming" ],
    "compilerCompatibility": {
        "kics2"   : ">= 3.0.0, < 4.0.0",
        "pakcs"   : ">= 3.0.0, < 4.0.0",
        "curry2go": ">= 1.0.0",
        "kmcc"    : ">= 0.1.0"
    },
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "dependencies": {
    },
    "testsuite": {
        "src-dir": "test",
        "modules":  [ "TestPrelude", "TestDataChar", "TestList",
                      "TestIORefs", "TestFunction",
                      "TestEnvironment", "TestTextShow",
                      "TestAllValues", "TestSetFunctions" ]
    },
    "source": {
      "git": "https://git.ps.informatik.uni-kiel.de/curry/curry-libs.git",
      "tag": "$version"
    }
}
